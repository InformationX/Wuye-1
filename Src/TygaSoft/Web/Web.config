<?xml version="1.0"?>
<!--
  有关如何配置 ASP.NET 应用程序的详细消息，请访问
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <configSections>
    <section name="rewriter" requirePermission="false" type="Intelligencia.UrlRewriter.Configuration.RewriterConfigurationSectionHandler, Intelligencia.UrlRewriter"/>
  </configSections>
  <connectionStrings>
    <add name="SqlProviderConnString" connectionString="Data Source=localhost;Integrated Security=SSPI;Initial Catalog=CollectLifeDb" providerName="System.Data.SqlClient"/>
    <add name="TygaAspnetDbConnString" connectionString="Data Source=localhost;Integrated Security=SSPI;Initial Catalog=CollectLifeAspnetDb" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="AESEncryptKey" value="CLife.2b313a97-d66e-48e0-b6f5-ae0afe7c946f"/>
    <add key="WebDAL" value="TygaSoft.SqlServerDAL,TygaSoft.SqlServerDAL"/>

    <!-- 上传文件根目录 -->
    <add key="FilesRoot" value="~/FilesRoot"/>
    <!-- 上传文件大小限制 -->
    <add key="UploadFileSize" value="5120000"/>
    <!-- 生成缩略图 -->
    <add key="PC" value="800*800,350*350,50*50"/>
    <add key="Android" value="350*350,150*150,60*60"/>
    <add key="IOS" value="350*350,150*150,60*60"/>
    <add key="IsCteateContentPictureThumbnail" value="1"/>
    <!-- Enable data caching -->
    <add key="EnableCaching" value="false"/>
    <add key="CacheDependencyAssembly" value="TygaSoft.TableCacheDependency,TygaSoft.TableCacheDependency"/>
    <add key="CacheDatabaseName" value="TygaDb"/>
    <add key="SysEnumCacheDuration" value="12"/>
    <add key="ProvinceCityCacheDuration" value="12"/>
    <add key="SysEnumTableDependency" value="SysEnum"/>
    <add key="ProvinceCityTableDependency" value="ProvinceCity"/>
    <!-- processing options (Asynch/Synch) -->
    <add key="StrategyAssembly" value="TygaSoft.BLL"/>
    <add key="OrderStrategyClass" value="TygaSoft.BLL.OrderAsynchronous"/>
    <!-- Msmq -->
    <add key="MsmqMessaging" value="TygaSoft.MsmqMessaging,TygaSoft.MsmqMessaging"/>
    <add key="OrdersMsmqPath" value="FormatName:DIRECT=OS:cafuc258-pc\private$\OrdersQueue"/>
    <!--<add key="OrdersMsmqPath" value="FormatName:DIRECT=OS:ay130916172011z\private$\OrdersQueue"/>-->
    <add key="EmailMsmqPath" value="FormatName:DIRECT=OS:cafuc258-pc\private$\EmailQueue"/>
    <!--<add key="EmailMsmqPath" value="FormatName:DIRECT=OS:ay130916172011z\private$\EmailQueue"/>-->
    <!--电子邮件发送模板-->
    <add key="EmailForRegister" value="~/Templates/Html/EmailForRegister.htm"/>
    <add key="EmailForResetPsw" value="~/Templates/Html/EmailForResetPsw.htm"/>
    <add key="WebSiteHost" value="http://m.tygaweb.com"/>
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
    <authentication mode="Forms">
      <forms defaultUrl="~/" loginUrl="~/login.html" name="CollectLifeAspNetApp" path="/" timeout="30" protection="All" slidingExpiration="true" requireSSL="false" cookieless="UseCookies"/>
    </authentication>
    <membership defaultProvider="MsSqlMembershipProvider" hashAlgorithmType="SHA1">
      <providers>
        <clear/>
        <add name="MsSqlMembershipProvider" type="System.Web.Security.SqlMembershipProvider" connectionStringName="TygaAspnetDbConnString" applicationName="CollectLifeWeb" requiresUniqueEmail="false" enablePasswordRetrieval="false" enablePasswordReset="true" requiresQuestionAndAnswer="false" passwordFormat="Hashed" minRequiredPasswordLength="6" minRequiredNonalphanumericCharacters="0" maxInvalidPasswordAttempts="15" passwordAttemptWindow="10" passwordStrengthRegularExpression="^([0-9a-zA-Z]+){6,30}$"/>
      </providers>
    </membership>
    <roleManager defaultProvider="MsSqlRoleProvider" enabled="true" cookieName="CollectLifeWebRoleCookie" cookiePath="/" cookieProtection="All" cookieRequireSSL="false" cookieSlidingExpiration="true" cookieTimeout="150" createPersistentCookie="false" cacheRolesInCookie="true" maxCachedResults="25">
      <providers>
        <clear/>
        <add name="MsSqlRoleProvider" connectionStringName="TygaAspnetDbConnString" applicationName="CollectLifeWeb" type="System.Web.Security.SqlRoleProvider"/>
      </providers>
    </roleManager>
    <anonymousIdentification enabled="false"/>
    <profile automaticSaveEnabled="false" enabled="false" defaultProvider="MsSqlProfileProvider" inherits="CustomProvider.CustomProfileCommon">
      <providers>
        <clear/>
        <add name="MsSqlProfileProvider" type="CustomProvider.MsSqlProfileProvider" connectionStringName="TygaAspnetDbConnString" applicationName="CollectLifeWeb" description="汇生活"/>
      </providers>
    </profile>
    <caching>
      <sqlCacheDependency enabled="false" pollTime="10000">
        <databases>
          <add name="CollectLifeDb" connectionStringName="SqlProviderConnString" pollTime="10000"/>
        </databases>
      </sqlCacheDependency>
    </caching>
    <siteMap defaultProvider="XmlSiteMapProvider" enabled="true">
      <providers>
        <add name="XmlSiteMapProvider" description="站点地图默认提供程序." type="System.Web.XmlSiteMapProvider" siteMapFile="Web.sitemap"/>
      </providers>
    </siteMap>
    <!--<pages>
      <controls>
        -->
    <!--<add tagPrefix="asp" namespace="Wuqi.Webdiyer" assembly="AspNetPager"/>-->
    <!--
      </controls>
    </pages>-->
    <httpModules>
      <add name="UrlRewriter" type="Intelligencia.UrlRewriter.RewriterHttpModule,Intelligencia.UrlRewriter"/>
    </httpModules>
  </system.web>
  <!--<system.webServer>
    <modules runAllManagedModulesForAllRequests="true">
      <add name="UrlRewriter" type="Intelligencia.UrlRewriter.RewriterHttpModule"/>
    </modules>
  </system.webServer>-->
  <rewriter file="~/App_Data/urlrewriter.xml"/>
  <!--<rewriter></rewriter>-->
  <system.serviceModel>
    <serviceHostingEnvironment multipleSiteBindingsEnabled="true" />
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>
